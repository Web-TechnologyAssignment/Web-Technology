extends layout
block content
    h1= title
    p Welcome to  #{title}
    form(name="search", action="/foursquare",method="GET")
        p
            | Search by Username
            = " "
            input(type="text",name="name")
        p
            | How many days?
            = " "
            input(type="text",name="day")
            p (5 days default)
        input(type="submit",value="submit")
    br

